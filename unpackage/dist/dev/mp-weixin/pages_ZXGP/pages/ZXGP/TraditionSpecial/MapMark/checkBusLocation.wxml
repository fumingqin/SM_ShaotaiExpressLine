<view class="content"><map class="map" style="{{'height:'+(mapHeight)+';'}}" id="map1" latitude="{{centerLatitude}}" longitude="{{centerLongitude}}" markers="{{markers}}" show-location="{{showLocation}}" scale="{{scale}}" polyline="{{polyline}}" controls="{{controls}}"></map><view class="orderInfo"><text style="font-size:32rpx;margin-left:30rpx;margin-top:20rpx;">{{orderInfo.station}}</text><text class="textView">{{"发车时间："+orderInfo.time}}</text><text class="textView">{{"当前所在站点："+orderInfo.currentStation}}</text><text class="textView">{{"下一站："+orderInfo.stationNum}}</text><text class="textView" style="margin-bottom:20rpx;">{{"预计时间："+orderInfo.dowStation}}</text></view></view>